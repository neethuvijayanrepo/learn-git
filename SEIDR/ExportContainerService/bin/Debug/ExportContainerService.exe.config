<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
  <appSettings>
    <!--Default connection timeout. Default command timeout for service procedures will be this times three-->
    <add key="Timeout" value="60"/>
    <!--Empty value will just use default value-->
    <add key="LoadBatch0TimeoutBuffer" value=""/>
    <!--Pause time in seconds when there aren't any facilities eligible for the current Export Group.-->
    <add key="NO_WORK_PAUSE_GROUP" value="20" />
    <!--Pause time in seconds when there aren't any facilities eligible to export for ANY Export Group-->
    <add key="NO_WORK_PAUSE_ALL" value="30" />
    <!--Number of executions to wait when usp_ExportGroup_PickNextFacility returns no work before logging that no work was found.  -->
    <add key="NoWorkStreakNotificationLimit" value="40"/>
    
    <!--Source connection information for pulling data from-->
    <add key="SourceDatabaseServer" value="NCIMTXDEVSE01"/>
    <add key="SourceDatabaseCatalog" value="Andromeda"/>

    <!--Database to export data to-->
    <add key="DestinationDatabaseServer" value="NCIMTXDEVSE01"/>
    <add key="DestinationDatabaseCatalog" value="Andromeda"/>
    <add key="DestinationUser" value=""/>
    <add key="DestinationPassword" value=""/>

    <!--DB for logging information - probably going to match Andromeda_Staging, at least at first-->
    <add key="LogDatabaseServer" value="NCIMTXDEVSE01"/>
    <add key="LogDatabaseCatalog" value="Andromeda"/>
    <add key="LoggingProcedure" value="ETL_META.usp_ExportLog_i"/>
    <!--
    Expected parameters for logging procedure:
      @LogMessage varchar(max)
      @ExportGroupID tinyint
      @ExportModelID int
      @MessageType varchar(20)
    
    -->
    <add key="LoggingSummaryProcedure" value="ETL_META.usp_ExportLog_GetSummary"/>
    <add key="ErrorSummaryTimeFrame" value="20" />
    <!--
      Number of days to wait before deleting data from export log. 
      Values that are empty or less than 1 will skip deleting
    -->
    <add key="AgedLogCleanupDays" value=""/>

    <!--
      If smtp server or mail domain are left out, mail will not be sent. 
      Also note that the mail is a very simple set up. Used for status emails and operation alerts
    -->
    <add key="MailSender" value="METRIX.ExportContainerService@navigant.com"/>
    <add key="SmtpServer" value="HYBRIDNCI.NAVIGANT.COM"/>
    <add key="MailDomain" value="navigant.com"/>
    <!--
      The MailTo address for startup/error notifications
    -->
    <add key="StatusMailTo" value="ryan.langer@navigant.com"/>

  </appSettings>
</configuration>
