<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
  <appSettings>
    <!--<add key="DatabaseServer" value="NCIHCTSTSQL07.nciwin.local\SQL2014"/>-->
    <add key="DatabaseServer" value="metaldev.cymetrix.com\SQL2014"/>
    <add key="DatabaseCatalog" value="MIMIR"/>
    <add key="Timeout" value="180"/>

    <add key="JobLibrary" value="C:\SEIDR.JobExecutor\Jobs\"/>
    <add key="LogRootDirectory" value="C:\SEIDR.JobExecutor\"/>

    <!--
      Interval (seconds) to perform long run check 
    -->    
    <add key="LongRunCheckInterval" value="150"/>
    <!--
      Number of Operation Executor threads to create, limit 15
    -->
    <add key="ThreadCount" value="1"/>
    <!--
      Number of Queue threads to create, limit 6
      -->
    <add key="QueueThreadCount" value="2"/>

    <!--  Minimum number of minutes to wait after a file is modified before it can be registered -->
    <add key="QueueCutoverTime" value="5"/>
    <!--
      Used as parameter for number of JobExecutions to grab at a time. Minimum 5.
      Higher value means fewer database hits, but potentially stale priorities.      
    -->
    <add key="BatchSize" value="5"/>
    
    <!--
      If smtp server or mail domain are left out, mail will not be sent. 
      Also note that the mail is a very simple set up. Used for status emails and operation alerts
    -->
    
    <add key="SenderDisplayName" value="SEIDR Job Executor"/>
    <add key="MailSender" value="SEIDR.JobExecutor@navigant.com"/>
    <add key="SmtpServer" value="HYBRIDNCI.NAVIGANT.COM"/>
    <add key="MailDomain" value="navigant.com"/>
    
    <!--
      The MailTo address for startup/error notifications
    -->
    <add key="StatusMailTo" value="ryan.langer@navigant.com"/>
  
    <!-- Configurations for preprocess job-->
    <add key="AndromedaReportServer" value="SRVRS01"/>
    
    <!-- LiveVox Export -->
    <add key="METRIX_ANDROMEDA_DATABASE_LOOKUP" value="LiveVox"/>
  </appSettings>
</configuration>
